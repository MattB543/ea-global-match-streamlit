# Streamlit Secrets Configuration
# Copy this file to secrets.toml and fill in your actual values

# Gemini API Key (required)
# Get your key from: https://aistudio.google.com/app/apikey
GEMINI_API_KEY = "your-gemini-api-key-here"

# Google Sheets CSV URL (required)
# Use the full Google Sheets URL, e.g.:
# https://docs.google.com/spreadsheets/d/YOUR_SHEET_ID/edit
CSV_URL = "https://docs.google.com/spreadsheets/d/YOUR_SHEET_ID/edit"

# App Password (required)
# Set a strong password to protect access to the app
# This can be any string you choose - keep it secret!
APP_PASSWORD = "your-secure-password-here"
